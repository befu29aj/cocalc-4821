\begin{document}

\textbf{Echelon Form and Complex Numbers}

{\aufgabe} Find the equation of the quadratic
  \begin{equation}
    \label{eq:kieshiec}
    y=ax^{2}+bx+c
  \end{equation}
  which passes through $(0,0),(1,2),(-1,3)$.
  
\end{document}

